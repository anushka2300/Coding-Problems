#include <stdio.h>

int main()
{
    int n,b[50]={0};
    scanf("%d",&n);
    int a[n];
    printf("original elements");
    for(int i=0;i<n;i++){
    scanf("%d",&a[i]);
    }
   
    for(int i=0;i<n;i++){
    b[i]=a[i];
    }
    printf("copied elements in b:");
    for(int i=0;i<n;i++){
    printf("%d",b[i]);
    }
    

    return 0;
}

