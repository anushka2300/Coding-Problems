#include <stdio.h>

int main()

{  int n,flag=0;
scanf("%d",&n);
int a[n][n];
    for(int i=0;i<n;i++){
    for(int j=0;j<n;j++){
    scanf("%d",&a[i][j]);
    }
    }
     for(int i=0;i<n;i++){
    for(int j=0;j<n;j++){
    if(j<i && a[i][j]!=0){
    if(a[i][j]!=0){
    flag=1;
    
    }
    
    }
    }}
    
    if(flag==1){
    printf("not upper triangle");
    }
    else
    printf("upper triangle");
    return 0;
}

   

